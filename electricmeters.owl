@prefix : <http://purl.org/daafse/electricmeters##> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix daafse: <http://purl.org/daafse/electricmeters#> .
@base <http://purl.org/daafse/electricmeters#> .

<http://purl.org/daafse/electricmeters#> rdf:type owl:Ontology ;
                                         
                                         owl:imports <http://purl.oclc.org/NET/ssnx/ssn> ;
                                         
                                         owl:versionIRI <http://purl.org/daafse/electricmeters/1.0.0> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/daafse/electricmeters#hasStream

daafse:hasStream rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has stream"@en ;
                 
                 rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#SensingDevice> .





#################################################################
#
#    Data properties
#
#################################################################


###  http://purl.org/daafse/electricmeters#hasSerialNumber

daafse:hasSerialNumber rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       
                       rdfs:label "has serial number"@en ;
                       
                       rdfs:domain daafse:Mercury230 ;
                       
                       rdfs:range rdfs:Literal .



###  http://purl.org/daafse/electricmeters##hasPhaseNumber

:hasPhaseNumber rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:subPropertyOf <http://www.loa-cnr.it/ontologies/DUL.owl#hasDataValue> ;
                
                rdfs:range xsd:integer .



###  http://purl.org/daafse/electricmeters##hasQuantityValue

:hasQuantityValue rdf:type owl:DatatypeProperty ;
                  
                  rdfs:subPropertyOf <http://www.loa-cnr.it/ontologies/DUL.owl#hasRegionDataValue> .





#################################################################
#
#    Classes
#
#################################################################


###  http://purl.org/daafse/electricmeters#ElectricMeter

daafse:ElectricMeter rdf:type owl:Class ;
                     
                     rdfs:label "ElectricMeter"@en ,
                                "Электросчетчик"@ru ;
                     
                     rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#SensingDevice> .



###  http://purl.org/daafse/electricmeters#ElectricityFeature

daafse:ElectricityFeature rdf:type owl:Class ;
                          
                          rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#hasProperty> ;
                                            owl:someValuesFrom daafse:VoltageProperty
                                          ] ;
                          
                          rdfs:comment "Электричество. Его свойства: напряжение, сила тока и т.д."@ru .



###  http://purl.org/daafse/electricmeters#Mercury230

daafse:Mercury230 rdf:type owl:Class ;
                  
                  rdfs:label "Mercury 230"@en ,
                             "Меркурий 230"@ru ;
                  
                  rdfs:subClassOf daafse:ElectricMeter ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#observes> ;
                                    owl:someValuesFrom daafse:VoltageProperty
                                  ] ;
                  
                  rdfs:seeAlso "http://77cs.ru/mercury-230"^^xsd:anyURI .



###  http://purl.org/daafse/electricmeters#PolyphaseVoltageObservation

daafse:PolyphaseVoltageObservation rdf:type owl:Class ;
                                   
                                   rdfs:subClassOf daafse:VoltageObservation ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#observationResult> ;
                                                     owl:allValuesFrom daafse:PolyphaseVoltageSensorOutput
                                                   ] .



###  http://purl.org/daafse/electricmeters#PolyphaseVoltageSensorOutput

daafse:PolyphaseVoltageSensorOutput rdf:type owl:Class ;
                                    
                                    rdfs:subClassOf daafse:VoltageSensorOutput .



###  http://purl.org/daafse/electricmeters#PolyphaseVoltageValue

daafse:PolyphaseVoltageValue rdf:type owl:Class ;
                             
                             rdfs:subClassOf daafse:VoltageValue ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasPhaseNumber ;
                                               owl:allValuesFrom xsd:integer
                                             ] .



###  http://purl.org/daafse/electricmeters#QuantityObservationValue

daafse:QuantityObservationValue rdf:type owl:Class ;
                                
                                rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#ObservationValue> .



###  http://purl.org/daafse/electricmeters#VoltageObservation

daafse:VoltageObservation rdf:type owl:Class ;
                          
                          rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#Observation> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#observationResult> ;
                                            owl:allValuesFrom daafse:VoltageSensorOutput
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#featureOfInterest> ;
                                            owl:allValuesFrom daafse:ElectricityFeature
                                          ] .



###  http://purl.org/daafse/electricmeters#VoltageProperty

daafse:VoltageProperty rdf:type owl:Class ;
                       
                       rdfs:label "Напряжение"@ru ;
                       
                       rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#Property> .



###  http://purl.org/daafse/electricmeters#VoltageSensorOutput

daafse:VoltageSensorOutput rdf:type owl:Class ;
                           
                           rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#SensorOutput> ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://purl.oclc.org/NET/ssnx/ssn#observationResult> ;
                                             owl:allValuesFrom daafse:VoltageValue
                                           ] .



###  http://purl.org/daafse/electricmeters#VoltageValue

daafse:VoltageValue rdf:type owl:Class ;
                    
                    rdfs:subClassOf daafse:QuantityObservationValue .





